      </Label>
      <Label fx:id="timeElapsed" layoutX="13.0" layoutY="121.0" text="Time elapsed: 0" />
      <Button fx:id="addWorkerThread" layoutX="250.0" layoutY="190.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="26.0" text="+">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="removeWorkerThread" layoutX="287.0" layoutY="190.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="26.0" text="-">
