<AnchorPane prefHeight="480.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.svega.vanitygen.fxmls.LaunchPage">
